{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "headers": [
      {
          "source": "*",
          "headers": [
              {
                  "key": "Access-Control-Allow-Origin",
                  "value": "*"
              }
          ]
      }
  ],
  "source": "functions"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    },
    "firestore": {
      "port": 8080
    }
  }
}
